<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefWidth="300.0" styleClass="loader-main-panel" stylesheets="@../resources/styles/theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ghirbal.controllers.DbLoaderController">
    <children>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="counterLbl" alignment="CENTER" maxHeight="1.7976931348623157E308" minWidth="140.0" styleClass="icm-24" text="0" />
                <VBox alignment="CENTER" prefWidth="200.0">
                    <children>
                        <fx:include fx:id="pi" source="progressIndicator.fxml" />
                        <Label fx:id="progressLbl" styleClass="p14" text="%LOAD_PERCENTAGE">
                            <font>
                                <Font size="15.0" />
                            </font>
                        </Label>
                    </children>
                    <padding>
                        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
                    </padding>
                </VBox>
            </children>
        </HBox>
    </children>
</VBox>
